            <field name="res_model">stock.picking</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form,calendar</field>
            <field name="domain">[('state','not in',('done','cancel'))]</field>
            <field name="context">{
                    'contact_display': 'partner_address',
            }
            </field>
            <field name="search_view_id" ref="stock.view_picking_internal_search"/>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click here to create a new transfer.
              </p><p>
                You can either do it immediately or mark it as Todo for future processing. Use your scanner to validate the transferred quantity quicker.
              </p>
            </field>
        </record>

        <record model="ir.ui.menu" id="stock.all_picking">
            <field name="name">Stock Operations</field>
            <field name="groups_id" eval="[(6,0,[ref('stock.group_stock_manager')])]"/>
            <field name="parent_id" ref="stock.menu_stock_root"/>
            <field name="sequence">1</field>
        </record>

        <record id="sakinah_all_picking_shop" model="ir.actions.act_window">
